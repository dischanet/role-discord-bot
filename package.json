{
  "name": "role-discord-bot",
  "version": "1.0.0",
  "description": "Discord Bot for role management.",
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "author": "Yusuke Hayashi <yusuke8h@gmail.com>",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -p . --outDir dist",
    "fix": "rm -rf node_modules package-lock.json && npm run update && npm run sort && npm i && npm run fmt && npm run lint && npm build",
    "fmt": "prettier --write .",
    "lint": "eslint --fix --ext .ts .",
    "sort": "npx sort-package-json",
    "start": "node ./dist/index.js",
    "update": "npx npm-check-updates -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run fmt && npm run lint"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "sequelize": "^5.21.13",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@types/sqlite3": "^3.1.6",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-unicorn": "^20.1.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "snowpack": "^2.5.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "type": "commonjs"
}
