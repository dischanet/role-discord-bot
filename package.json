{
  "name": "role-discord-bot",
  "version": "1.0.0",
  "description": "Discord Bot for role management.",
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "author": "Yusuke Hayashi <yusuke8h@gmail.com>",
  "main": "index.ts",
  "scripts": {
    "build": "tsc --outDir dist",
    "fix": "rm -rf node_modules package-lock.json && npm run update && npm run sort && npm i && npm run fmt && npm run lint",
    "fmt": "prettier --write .",
    "lint": "eslint --fix --ext .ts .",
    "sort": "npx sort-package-json",
    "start": "node ./dist/index.js",
    "update": "npx npm-check-updates -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run fmt && npm run lint"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@types/node": "^14.0.9",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.1.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  }
}
